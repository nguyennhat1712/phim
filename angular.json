{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "DuAnPhimFrontEnd06": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/DuAnPhimFrontEnd06",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",

              "adminlayout/vendor/bootstrap/css/bootstrap.min.css",
              "adminlayout/vendor/font-awesome/css/font-awesome.min.css",
              "adminlayout/vendor/datatables/dataTables.bootstrap4.css",
              "adminlayout/css/sb-admin.css"
            ],
            "scripts": [

              "node_modules/popper.js/dist/popper.min.js",

              "adminlayout/vendor/jquery/jquery.min.js",
              "adminlayout/vendor/bootstrap/js/bootstrap.bundle.min.js",
              "adminlayout/vendor/jquery-easing/jquery.easing.min.js",
              "adminlayout/vendor/chart.js/Chart.min.js",
              "adminlayout/vendor/datatables/jquery.dataTables.js",
              "adminlayout/vendor/datatables/dataTables.bootstrap4.js",
              "adminlayout/js/sb-admin.min.js",
              "adminlayout/js/sb-admin-datatables.min.js",
              "adminlayout/js/sb-admin-charts.min.js",
              "./node_modules/bootstrap/js/src/modal.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "DuAnPhimFrontEnd06:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "DuAnPhimFrontEnd06:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "DuAnPhimFrontEnd06:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "DuAnPhimFrontEnd06-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "DuAnPhimFrontEnd06:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "DuAnPhimFrontEnd06"
}